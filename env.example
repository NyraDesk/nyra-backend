# N8N Configuration
# Copia questo file in .env e configura i valori corretti

# URL del webhook per Google Calendar (produzione)
N8N_URL=http://localhost:5678
VITE_N8N_URL=http://localhost:5678

# Debug n8n (1=abilitato, 0=disabilitato)
VITE_NYRA_DEBUG_N8N=1

# Development fallback (commenta per produzione)
# N8N_URL=https://your-n8n-production-url.com
# VITE_N8N_URL=https://your-n8n-production-url.com

# Altre configurazioni
NYRA_ENABLE_AUTOMATION=1

# Timezone di default per NYRA
VITE_DEFAULT_TIMEZONE=Europe/Rome

# Google OAuth2 Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=nyra://oauth/callback

# OAuth Database
OAUTH_DATABASE_PATH=nyra-oauth.db

# Security
OAUTH_ALLOWED_IPS=127.0.0.1,::1
OAUTH_RATE_LIMIT_WINDOW_MS=900000
OAUTH_RATE_LIMIT_MAX_REQUESTS=100

# Deepgram Voice Recognition API
VITE_DEEPGRAM_API_KEY=your_deepgram_api_key_here
